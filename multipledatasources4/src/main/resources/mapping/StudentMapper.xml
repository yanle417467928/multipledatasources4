<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.mzd.multipledatasources.mapper.StudentMapper">
    <insert id="save" parameterType="com.mzd.multipledatasources.bean.Student">
        insert into student(student_name, class_id, score)
        values (#{studentName}, #{classId}, #{score})
    </insert>
</mapper>